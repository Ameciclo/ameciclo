{
  "kind": "collectionType",
  "collectionName": "workgroups",
  "info": {
    "name": "Grupos de Trabalho",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "telegram_id": {
      "type": "biginteger"
    },
    "icon": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "telegram_url": {
      "type": "string"
    },
    "projects": {
      "collection": "project",
      "via": "workgroup"
    },
    "description": {
      "type": "text"
    },
    "Links": {
      "type": "component",
      "repeatable": true,
      "component": "button.links"
    },
    "folder_url": {
      "type": "string"
    },
    "botmembers": {
      "via": "workgroups",
      "collection": "botmembers"
    },
    "washere": {
      "via": "oncethere",
      "collection": "botmembers"
    }
  }
}
