{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "media": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "goal": {
      "type": "text"
    },
    "endDate": {
      "type": "date"
    },
    "description": {
      "type": "text"
    },
    "startDate": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "ongoing",
        "finished",
        "paused"
      ]
    },
    "bikeCulture": {
      "type": "enumeration",
      "enum": [
        "high",
        "low",
        "medium"
      ]
    },
    "instArticulation": {
      "type": "enumeration",
      "enum": [
        "high",
        "low",
        "medium"
      ]
    },
    "politicIncidence": {
      "type": "enumeration",
      "enum": [
        "high",
        "low",
        "medium"
      ]
    },
    "slug": {
      "type": "string"
    },
    "isHighlighted": {
      "type": "boolean"
    },
    "gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
